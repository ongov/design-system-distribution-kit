{
    "private": true,
    "name": "ontario-design-system",
    "version": "0.8.4",
    "description": "A repository of ODS front-end and UI code, including a pattern library, templates, and misc components, fonts for designers",
    "main": "index.js",
    "scripts": {
        "test": "test",
        "test-build": "serve fractal/build",
        "dev": "gulp --lang=en",
        "dev:fr": "gulp --lang=fr",
        "build-react": "cd reactjs && npm run build && cd ..",
        "build-react-ci": "cd reactjs && npm ci && npm run build && cd ..",
        "build:en": "gulp build --lang=en",
        "build:fr": "gulp build --lang=fr",
        "build-ci-dev-en": "gulp -v && npm run build-react-ci && gulp build --lang=en --sandbox=ui-system",
        "build-ci-dev-fr": "gulp -v && npm run build-react-ci && gulp build --lang=fr --sandbox=ui-system",
        "build-ci-en": "gulp -v && npm run build-react-ci && gulp build --lang=en",
        "build-ci-fr": "gulp -v && npm run build-react-ci && gulp build --lang=fr",
        "serve": "serve fractal/build",
        "serve-dist": "serve dist",
        "deploy-dani": "aws s3 sync fractal/build s3://sb-designsystem.s3.vpc.ont/dani"
    },
    "repository": {
        "type": "git",
        "url": "[git@git.ssh.ontariogovernment.ca:8080]:frontend/ui-system.git"
    },
