        "cheerio": "^1.0.0-rc.3",
        "child_process": "^1.0.2",
        "del": "^5.1.0",
        "dotenv": "^8.2.0",
        "fs": "0.0.1-security",
        "glob": "^7.1.6",
        "gulp": "^4.0.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-cheerio": "^1.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-cli": "^2.0.1",
